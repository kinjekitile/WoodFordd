@model BranchContactViewModel
@{
    ViewBag.Title = Model.Branch.BranchContactPageTitle;
    ViewBag.MetaKeywords = Model.Branch.BranchContactMetaKeywords;
    ViewBag.MetaDescription = Model.Branch.BranchContactMetaDescription;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="twelve columns">
        <h1>Contact</h1>
        <p>Thank you for contacting the @Model.Branch.Title branch. We will be in touch with you shortly.</p>
    </div>
</div>
