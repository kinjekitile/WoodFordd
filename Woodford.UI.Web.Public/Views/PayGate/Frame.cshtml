@model Woodford.Infrastructure.PayGatePaymentProcessor.paygateService.CardPaymentResponseType
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<form action="@(Model.Redirect.RedirectUrl)" name="frmSubmit" id="frmSubmit" method="post" target="iframeID">
    @foreach (var item in Model.Redirect.UrlParams) {
        <input type="hidden" name="@item.key" value="@item.value" />
    }
    <input type="submit" value="submit" />
</form>
<iframe src="" id="iframeID" name="iframeID" scrollbar="auto" style="width: 600px;height: 800px;"></iframe>

@section scripts {

    <script type="text/javascript">

        $(document).ready(function () {
            $("#frmSubmit").submit();
        });


        function close3DMessageAndSubmitForm(resId) {

            window.location = "Complete?resId=" + resId;
        }

    </script>
}

