<html lang="en">
<!--<![endif]-->
<head>

    @*<meta name="robots" content="noindex,nofollow" />*@
    <meta charset="utf-8">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=10,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="keywords" content="@ViewBag.MetaKeywords" />
    <meta name="description" content="@ViewBag.MetaDescription" />
    <meta name="author" content="Goodkid New Media Studio" />
    <meta http-equiv="Content-Language" content="en-us">
    @{
        string title = ViewBag.Title;
        if (string.IsNullOrEmpty(title)) {
            title = "Woodford Vehicle Hire";
        } else {
            title = title + " | Woodford Vehicle Hire";
        }
    }
    <title>@title</title>

    <link href="//fonts.googleapis.com/css?family=Raleway:400,300,600" rel="stylesheet" type="text/css">
    <link href='//fonts.googleapis.com/css?family=Open+Sans:400,700' rel='stylesheet' type='text/css'>
    <link href='//fonts.googleapis.com/css?family=Oswald:400' rel='stylesheet' type='text/css'>

    <link rel="stylesheet" href="@Url.Content("~/Content/css/styles.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/css/progress.css")">
 
    <link rel="stylesheet" href="@Url.Content("~/Content/flexslider/flexslider.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/slickNav/slickNav.css")">
    <link rel="stylesheet" media="screen" type="text/css" href="@Url.Content("~/Content/css/datepicker.css")" />
    

    <script type="text/javascript" src="@Url.Content("~/Scripts/modernizr-custom.js")"></script>

    <script type="text/javascript" src="@Url.Content("~/Scripts/jquery-1.11.0.min.js")"></script>
    @*<script type="text/javascript" src="@Url.Content("~/Scripts/jquery-ui-1.8.24.min.js")"></script>*@
    <script type="text/javascript" src="@Url.Content("~/Scripts/jquery.simplemodal.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/slickNav/jquery.slicknav.min.js")"></script>

    <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
    


    <!--[if lt IE 9]>
        <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->

    <link rel="shortcut icon" href="@Url.Content("~/Content/_images/icons/favicon.ico")">

    <!-- TrustBox script -->
    <script type="text/javascript" src="https://widget.trustpilot.com/bootstrap/v5/tp.widget.sync.bootstrap.min.js" async></script>
    <!-- End Trustbox script -->

    @RenderSection("HeadIncludes", false)

</head>
<body>

    <div class="user-panel">
        <div class="container">
            <div class="row">
                <div class="twelve columns">
                    @*@Html.Partial("_LoginPartial")*@
                    @{ Html.RenderAction("LoginPartial", "Home"); }
                </div>
            </div>
        </div>

    </div>
    <div class="header">

        <div class="container">
            <div class="row">
                @*<div class="five columns">
                        <a id="brandmark" href="@Url.Action("Index", "Home")">
                            <img src="@Url.Content("~/Content/images/brandmark.png")" alt="" />
                        </a>
                    </div>*@
                <div class="twelve columns">
                    @{Html.RenderAction("Navigation", "Home"); }

                </div>
            </div>


            <div class="mobile-view">
                <a id="brandmark-mobile" href="@Url.Action("Index", "Home")">
                    <img src="@Url.Content("~/Content/images/brandmark.png")" alt="" />
                </a>
            </div>
            <div class="mobile-nav-wrapper"></div>
        </div>
    </div>

    @if (IsSectionDefined("searchpanel")) {
        @RenderSection("searchpanel", required: false)
    } else {
        ViewBag.PanelMinimised = true;
        Html.RenderPartial("_SearchPanelPartial", SearchCriteriaViewModelFactory.CreateInstance());
    }

    @RenderSection("herospace", required: false)

    <section>
        <div class="container main-content">
            @RenderBody()
        </div>
    </section>
    @Html.Partial("_FooterPartial")

    @if (IsSectionDefined("trustpilot")) {
        @RenderSection("trustpilot", required: false)
    }
    else {



        @*<div class="container-sub-footer">
            <div class="container">
                <div class="row">
                    <!-- TrustBox widget -->
                    <div class="trustpilot-widget" data-locale="en-GB" data-template-id="539ad0ffdec7e10e686debd7" data-businessunit-id="54620ca500006400057b77fe" data-style-height="350px" data-style-width="100%" data-theme="light" data-stars="4,5">
                        <a href="https://uk.trustpilot.com/review/www.woodford.co.za" target="_blank">Trustpilot</a>
                    </div>
                    <!-- End TrustBox widget -->
                </div>
            </div>
        </div>*@

       
    }




    

    @*@Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")*@

    <script>
        $(function () {
            $('#menu').slicknav({
                prependTo: '.mobile-nav-wrapper',
                allowParentLinks: true,
                label: ""
            });
        });
    </script>

    <script type="text/javascript" src="@Url.Content("~/Scripts/jquery.unobtrusive-ajax.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/jquery.scrollTo.min.js")"></script>

    <script type="text/javascript" src="@Url.Content("~/Scripts/datepicker.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/js/navigation.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/js/searchFunctions.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/js/functions.js")"></script>

    @if (IsSectionDefined("scrollToContent")) {
        @RenderSection("scrollToContent", required: false)
    } else {
        if (IsSectionDefined("searchpanel")) {
            Html.RenderPartial("_ScrollToMainContent");
        }
    }

    <!-- Google Analytics -->
    <script>
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date(); a = s.createElement(o),
            m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
        })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

        ga('create', 'UA-5859441-1', 'auto');  // Replace with your property ID.
        ga('require', 'displayfeatures');
        ga('send', 'pageview');

    </script>
    <!-- End Google Analytics -->

    @RenderSection("scripts", required: false)

    
</body>
</html>
